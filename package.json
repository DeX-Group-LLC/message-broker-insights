{
  "name": "mb-insights",
  "version": "1.0.0",
  "description": "A web-based dashboard for monitoring and analyzing NABHAS-compliant Message Brokers. Provides real-time insights into message flow, performance, and system health. Compatible with `mb-server-node` and other NABHAS implementations.",
  "author": "DeX Group, LLC",
  "license": "Apache-2.0",
  "homepage": "https://github.com/DeX-Group-LLC/mb-insights#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeX-Group-LLC/mb-insights.git"
  },
  "bugs": {
    "url": "https://github.com/DeX-Group-LLC/mb-insights/issues"
  },
  "keywords": [
    "message-broker",
    "message-broker-insights",
    "nabhas",
    "dashboard",
    "insights",
    "monitoring",
    "analytics"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --code-coverage --browsers=ChromeHeadless",
    "test:browser": "ng test --no-watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.3",
    "@angular/cdk": "^19.1.1",
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/forms": "^19.1.3",
    "@angular/material": "^19.1.3",
    "@angular/platform-browser": "^19.1.3",
    "@angular/platform-browser-dynamic": "^19.1.3",
    "@angular/router": "^19.1.3",
    "chart.js": "^4.4.7",
    "chartjs-adapter-moment": "^1.0.1",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "uuid": "^11.0.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.3",
    "@types/chart.js": "^2.9.41",
    "@types/jasmine": "~5.1.5",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.3"
  }
}
